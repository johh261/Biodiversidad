<script type=text/javascript>

$('.pictureframe').css('height', $('.pictureframe').find('div.image').outerHeight());


function createSet()
{
	axios.post('/api/sets').then((response) => {
		if (data = response.data) {
			$('#create-set').addClass('d-none');
			$('#clear-set').removeClass('d-none');
			$('input[name=set_id]').val(data.key);
		}
	})
}

function clearSet()
{
	$('#clear-set').addClass('d-none');
	$('#create-set').removeClass('d-none');
	$('input[name=set_id]').val(null);
}

function reset()
{
	$('#picture').off('load');
	$('#picture').attr('src', 'https://cache.moviestillsdb.com/i/500x/sbrjmcyu/machete-lg.jpg');
	
	$('#loading-preview').css('display', 'none');
	$('#showing-preview').css('display', 'none');
	$('#picture').attr('rel', '');
	clearTimeout(loadingPreview);
}

</script>

